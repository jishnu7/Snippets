
# Insert unicode char
ctrl-v u20b9

# Insert tab
ctrl-v ctrl-i

# Change file type
setfiletype xml

# convert tabs to spaces
set expandtab
retab!

# spaces to tab
set noexpandtab
retab!

#spell check
set spell
]s next
[s previous
z= suggestions
zg add word to dictionary
zw mark as wrong
zug/zuw undo

